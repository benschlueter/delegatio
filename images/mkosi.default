[Distribution]
Distribution=arch

[Output]
Format=disk
Bootable=yes
QCow2=yes
Output=image.qcow2
WorkspaceDirectory=/tmp

[Packages]
Packages=
         openssh
         vim
         curl
         
         socat
         ethtool
         conntrack-tools
         iptables-nft
         runc
         cri-o

         nfs-utils

[Validation]
Password=password

[Content]
Cache=.cache
Autologin=yes

[Host]
QemuHeadless=yes
